{
	"name": "equix",
	"version": "1.0.1",
	"private": true,
	"scripts": {
		"postinstall": "sed -i '' 's/#import <RCTAnimation\\/RCTValueAnimatedNode.h>/#import \"RCTValueAnimatedNode.h\"/' ./node_modules/react-native/Libraries/NativeAnimation/RCTNativeAnimatedNodesManager.h && jetifier -r && patch-package",
		"pre-build-android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
		"pre-build-ios": "react-native bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --assets-dest ios/assets",
		"build-web-script": "node ./script.js",
		"ios": "react-native run-ios",
		"android": "react-native run-android",
		"build-test-debug": "detox build --configuration ios.sim.debug",
		"test-debug": "chmod +x update-config-detox-test.sh && ./update-config-detox-test.sh && detox test --configuration ios.sim.debug",
		"build-test-release": "detox build --configuration ios.sim.release",
		"test-release": "detox test --configuration ios.sim.release",
		"quick-test": "npm run build-test-debug && npm run test-debug",
		"quick-test-release": "npm run build-test-release && npm run test-release",
		"react-devtools": "react-devtools",
		"rc-start": "npm start -- --reset-cache",
		"clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
		"clean-yarn": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean",
		"clean-start": "npm run clean && npm run rc-start",
		"fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean && npm install",
		"yarn-install": "rm -rf node_modules/  && rm -rf ios/Pods && rm -rf $TMPDIR/react-* && rm -rf ios/build/ModuleCache/* && yarn --ignore-engines && cd ios/ && pod install  && watchman watch-del-all && npm cache clean --force",
		"fresh-start": "npm run fresh-install && npm run rc-start",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"lint": "esw index.ios.js index.android.js src",
		"lint-fix": "esw index.ios.js index.android.js src utils __tests__/screens/login --fix",
		"lint:watch": "npm run lint -- --watch",
		"map-nestedScroll": "wml add ../react-native-nestedScroll ./node_modules/react-native-nestedScroll",
		"commit-code": "chmod +x commit-build.sh && ./commit-build.sh",
		"absa_prod_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh absa prod rebuildBundle",
		"absa_prod_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh absa prod",
		"hl_dev_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh hl dev rebuildBundle",
		"hl_dev_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh hl dev",
		"hl_prod_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh hl prod rebuildBundle",
		"hl_uat_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh hl uat",
		"hl_uat_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh hl uat rebuildBundle",
		"hl_prod_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh hl prod",
		"iressv4_dev_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh iressv4 dev rebuildBundle",
		"iressv4_dev_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh iressv4 dev",
		"iressv4_uat_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh iressv4 uat rebuildBundle",
		"iressv4_uat_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh iressv4 uat",
		"iressv4_prod_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh iressv4 prod rebuildBundle",
		"iressv4_prod_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh iressv4 prod",
		"cimb_dev_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh cimb dev rebuildBundle",
		"cimb_dev_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh cimb dev",
		"cimb_uat_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh cimb uat rebuildBundle",
		"cimb_uat_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh cimb uat",
		"cimb_prod_config": "chmod +x ./cicd/build.sh && ./cicd/build.sh cimb prod rebuildBundle",
		"cimb_prod_codepush": "chmod +x ./cicd/codepush.sh && ./cicd/codepush.sh cimb prod"
	},
