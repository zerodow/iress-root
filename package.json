	"detox": {
		"specs": "e2e",
		"configurations": {
			"android.emu.debug": {
				"binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
				"build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
				"type": "android.emulator",
				"name": "Nexus_6_API_27"
			},
			"android.emu.release": {
				"binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
				"build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
				"type": "android.emulator",
				"name": "Nexus_6_API_27"
			},
			"ios.sim.release": {
				"binaryPath": "ios/build/Build/Products/Release-iphonesimulator/equix.app",
				"build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/equix.xcworkspace -scheme equix -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
				"type": "ios.simulator",
				"name": "iPhone 6"
			},
			"ios.sim.debug": {
				"binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/equix.app",
				"build": "xcodebuild -workspace ios/equix.xcworkspace -scheme equix -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
				"type": "ios.simulator",
				"name": "iPhone 6"
			}
		}
